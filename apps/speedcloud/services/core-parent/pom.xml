<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <packaging>pom</packaging>

    <parent>
        <groupId>com.yunkang.saas.starter</groupId>
        <artifactId>saas-biz-starter-parent </artifactId>
        <version>1.0.1-SNAPSHOT</version>
    </parent>
    <groupId>net.aicoder.speedcloud.service.core</groupId>
    <artifactId>core-parent</artifactId>    <version>1.0.0-SNAPSHOT</version>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.yunkang.saas.platform.manage</groupId>
                <artifactId>manage-model</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.yunkang.saas.bootstrap</groupId>
                <artifactId>common-parent</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.yunkang.saas.bootstrap</groupId>
                <artifactId>bootstrap-monitor</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>com.yunkang.saas.bootstrap</groupId>
                <artifactId>bootstrap-jms</artifactId>
                <version>1.0.0-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>net.aicoder.speedcloud.service.core</groupId>
                <artifactId>core-model</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>net.aicoder.speedcloud.service.core</groupId>
                <artifactId>core-business</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>net.aicoder.speedcloud.service.core</groupId>
                <artifactId>core-client</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>net.aicoder.speedcloud.service.core</groupId>
                <artifactId>core-adapter-jenkins</artifactId>
                <version>${project.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <dependencies>
        <dependency>
            <groupId>com.yunkang.saas.common</groupId>
            <artifactId>common-utils</artifactId>
        </dependency>
        <dependency>
            <groupId>com.yunkang.saas.common</groupId>
            <artifactId>common-common</artifactId>
        </dependency>
        
    </dependencies>
    <profiles>
        <!-- 默认 -->
        <profile>
            <id>default</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <skipTests>true</skipTests>
            </properties>
            <modules>
                <module>model</module>
                <module>web</module>
                <module>client</module>
                <module>business</module>
                <module>jenkins</module>
            </modules>
        </profile>
        <profile>
            <id>client</id>
            <properties>
                <skipTests>true</skipTests>
            </properties>
            <modules>
                <module>model</module>
                <module>client</module>
            </modules>
        </profile>
    </profiles>
    <repositories>
        <repository>
            <id>my-nexus-releases</id>
            <name>Local Nexus Repository</name>
            <url>http://121.41.14.24:8081/nexus/content/groups/public/</url>
        </repository>
    </repositories>
</project>